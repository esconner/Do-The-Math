{
    "identifier": "joshgreco.DoTheMath",
    "name": "Do The Math",
    "organization": "Josh Greco",
    "description": "An expression evaluator for Nova.",
    "version": "1.0",
    "categories": ["commands"],
    "license": "MIT",
    "main": "main.js",

    "commands": {
        "editor" : [
          {
            "command" : "do-the-math.compute-and-append",
            "title" : "Evaluate",
            "when" : "editorHasSelection"
          },
          {
            "command" : "do-the-math.compute-and-replace",
            "title" : "Evaluate and Replace",
            "when" : "editorHasSelection"
          }
        ]
    },
    "bugs": "https://github.com/joshgreco/Do-The-Math/issues",
    "repository": "https://github.com/joshgreco/Do-The-Math"
}
